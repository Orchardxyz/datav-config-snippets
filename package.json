{
  "name": "datav-config-snippets",
  "displayName": "datav-config-snippets",
  "description": "A tool helps to generate basic datav configs snippets.",
  "version": "0.0.2",
  "license": "MIT",
  "publisher": "Orchard",
  "icon": "https://sm.ms/image/6jWqm2knAYbRye3",
  "repository": "https://github.com/jgyyz/datav-config-snippets",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:datav-config-snippets.datavConfigSnippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./snippets/datav.configs.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
